@model G_Stock_Vente.ViewModels.UniteVm

@{
    ViewData["Title"] = "G-Stock|Liste des Unites";
}

<!-- Breadcomb area Start-->
<div class="breadcomb-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcomb-list">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="fa fa-table-list"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>Liste des unites</h2>
                                    <p>Bienvenue <span class="bread-ntd">Gestion de stock et de Vente</span></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-3">
                            <div class="breadcomb-report">
                                <a onclick="showInPopup('@Url.Action("AddOrEdit", "Unite", null, Context.Request.Scheme)', 'Nouvel Unité')" class="btn">
                                    <i class="fa fa-circle-plus"> Nouvel Unité</i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcomb area End-->

<div id="view-all-unite">
    @await Html.PartialAsync("_ViewAllUnite", Model);
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}